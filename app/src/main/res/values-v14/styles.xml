        <!-- Window atribute -->
        <item name="android:windowTitleStyle">@style/MyWidowTitleStyle</item>

        <item name="android:windowBackground">@android:color/white</item>
        <item name="android:colorBackground">@android:color/white</item>
        <item name="android:colorForeground">@android:color/holo_blue_bright</item>
        <item name="android:colorForegroundInverse">@android:color/holo_blue_dark</item>

        <!-- Text color -->
        <item name="android:textColorPrimary">@android:color/black</item>
        <item name="android:textColorSecondary">@android:color/white</item>



        <item name="android:textViewStyle">@style/MyTextWiew</item>
        <item name="android:textAppearance">@style/MyTextApperanse</item>

    </style>

    <style name="MyTextWiew" parent="@android:style/Widget.TextView">
        <item name="android:textColor">#1e0ece</item>
    </style>

    <style name="MyTextApperanse" parent="@android:style/TextAppearance">
        <item name="android:textColor">#1e0ece</item>
    </style>

    <style name="MyWidowTitleStyle">
    </style>


</resources>